{
  "name": "tailwind-starter-template",
  "version": "1.0.0",
  "description": "starter project for building in HTML + CSS w. TailwindCSS",
  "main": "tailwind.config.js",
  "dependencies": {
    "tailwindcss": "^3.0.24"
  },
  "devDependencies": {},
  "scripts": {
    "dev": "npx tailwindcss -i src/style.css -o public/css/style.css --watch",
    "prod": "npx tailwindcss -i src/style.css -o public/css/style.css --minify"
  },
